{
  "manifest_version": 3,
  "name": "Save Current Site",
  "version": "1.0",
  "description": "Save the current site to the database.",
  "permissions": [
    "activeTab",
    "background"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
